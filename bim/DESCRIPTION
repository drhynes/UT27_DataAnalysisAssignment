Package: bim
Title: Brain Immune Migration (bim)
Version: 0.1.0
Authors@R: c(
    person(given = "Joyradyn", family = "James-Rollins", role = c("aut", "cre"),
           email = "jjamesrollins237@utexas.edu"),
    person(given = "Andrew", family = "Hynes", role = "aut",
           email = "drew.hynes@utexas.edu"),
    person(given = "Stephen", family = "Collins", role = "aut",
           email = "sacollins@utexas.edu")
